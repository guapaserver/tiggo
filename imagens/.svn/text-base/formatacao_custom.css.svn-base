
body
{
	background-image: url(../../imagens/site/fundo.png);
	margin: 0px;
	display:block;
	padding:0;
	background-position: top;
}

/*MENU TOPO*/
#largura_menu_topo
{
	font-family: 'DINNeuzeitGroteskStdBoldCnRg';
	top:0px;
	height:27px;
	background: #f4f1e1;
	display:block;
	border-top-width: thick;
	border-top-style: solid;
	border-top-color: #fff;
	border-top-width: 1px;
}

/*BANNER*/
#largura_banner
{
	display:block;
	min-height:160px;
	max-height:160px;
	background-color:#fff;
}

#logo 
{
	display:block;
	/*width:282px;*/
	width:100%;
	margin:0 auto;	
	float:left;
	height:116px;/*84 + 25 + 7*/
}

#logo  img
{
	display:block;
	/*width:282px;	*/
	margin-top:25px;
	margin-bottom:7px;
	
	/*centra a imagem*/
	left:50%;
	margin-left:-141px;	
	position:relative;
	/*fin centra img*/
	
	/*float:right;*/
}

#largura_banner hr
{
	display:block;
	width:100%;
	border-width: 0;
	height: 1px;
	border-top: 1px solid #e7e0b3;
	margin:1px;

	float:left;
}

/*MENU CATEGORIA*/
#conteudo_sub_menu{
	display:block;
	font-family: 'DINNeuzeitGroteskStdBoldCnRg';
	width:100%;
	height: 39px;
	float:left;
}

/*FIM MENU CATEGORIA*/

/*BASE BANNER*/
